class Main {
  public static void main(String[] args) {
    int a, b, c, delta, d, b2, v2;
    a=2;
    b=1;
    c=0;
    d=-4;
    b2=b*b;
    v2=((d)*(a)*(c));
    delta=(b2)+(v2);
    double r1, r2;
    r1=(-b+Math.sqrt(delta))/(2*a);
    r2=(+b+Math.sqrt(delta))/(2*a);
    System.out.println("A raiz do x' :" + r1);
    System.out.println("A raiz do x'' :" + r2);
    }
}

